@startuml

class Pessoa {
    - id: Long
    - nomeCompleto: String
    - dataNascimento: Date
    - genero: String
    - estrangeiro: Boolean

    - CPF: String
    - RG: String
    - outrosDocumentos: String

    - email: String
    - telefoneCelular: String
    - telefoneFixo: String

    - endereco: Endereco

    - escolaridade: Escolaridade
